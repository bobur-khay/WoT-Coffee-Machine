#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/c/Users/Z0057B3H/Desktop/WoT tutorial/node_modules/.pnpm/json-placeholder-replacer@2.1.2/node_modules/json-placeholder-replacer/dist/node_modules:/mnt/c/Users/Z0057B3H/Desktop/WoT tutorial/node_modules/.pnpm/json-placeholder-replacer@2.1.2/node_modules/json-placeholder-replacer/node_modules:/mnt/c/Users/Z0057B3H/Desktop/WoT tutorial/node_modules/.pnpm/json-placeholder-replacer@2.1.2/node_modules:/mnt/c/Users/Z0057B3H/Desktop/WoT tutorial/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/c/Users/Z0057B3H/Desktop/WoT tutorial/node_modules/.pnpm/json-placeholder-replacer@2.1.2/node_modules/json-placeholder-replacer/dist/node_modules:/mnt/c/Users/Z0057B3H/Desktop/WoT tutorial/node_modules/.pnpm/json-placeholder-replacer@2.1.2/node_modules/json-placeholder-replacer/node_modules:/mnt/c/Users/Z0057B3H/Desktop/WoT tutorial/node_modules/.pnpm/json-placeholder-replacer@2.1.2/node_modules:/mnt/c/Users/Z0057B3H/Desktop/WoT tutorial/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../../json-placeholder-replacer@2.1.2/node_modules/json-placeholder-replacer/dist/index.js" "$@"
else
  exec node  "$basedir/../../../../../../json-placeholder-replacer@2.1.2/node_modules/json-placeholder-replacer/dist/index.js" "$@"
fi
